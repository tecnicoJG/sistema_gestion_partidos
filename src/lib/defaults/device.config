# Factory Default Device Configuration
# This file contains the default settings for court controller devices
# Used for factory resets and initial setup

[device]
deviceFamily = 01
deviceId =
status = setup
locale = en

[sports]
available =

[network]
mode = ap
ssid =
security = open
ip = 192.168.4.1
subnet = 255.255.255.0

[theme]
default = light
